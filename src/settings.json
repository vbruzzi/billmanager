{
    "launch": {
        "configurations": [
        {
        "name": "(gdb) launch Linux",
        "type": "cppdbg",
        "request": "launch",
        "program": "/mnt/d/dev/a.out",
        "args": ["-fThreading"],
        "stopAtEntry": false,
        "cwd": "/mnt/d/dev",
        "environment": [],
        "externalConsole": true,
        "windows": {
        "MIMode": "gdb",
        "setupCommands": [
        {
        "description": "Enable pretty-printing for gdb",
        "text": "-enable-pretty-printing",
        "ignoreFailures": true
        }
        ]
        },
        "pipeTransport": {
        "pipeCwd": "",
        "pipeProgram": "c:\\windows\\sysnative\\bash.exe",
        "pipeArgs": ["-c"],
        "debuggerPath": "/usr/bin/gdb"
        },
        "sourceFileMap": {
        "/mnt/d": "d:\\"
        }
        }]
    }
}